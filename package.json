{
  "name": "effects-as-data",
  "version": "3.0.17",
  "description": "A micro abstraction layer for Javascript that makes writing, testing, and monitoring side-effects easy.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/orourkedd/effects-as-data"
  },
  "scripts": {
    "deploy": "npm run transpile && npm test, && npm publish; rm -rf es5",
    "prettier": "prettier --write {src/*.js,src/**/*.js}",
    "test": "jest",
    "transpile": "babel src --out-dir es5 --source-maps"
  },
  "author": "orourkedd",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lodash": "^4.17.11",
    "prettier": "^1.14.3"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/test.js",
      "<rootDir>/src/test.js",
      "<rootDir>/es5/test.js"
    ]
  }
}
